<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/ic_dash_training_flashcard_pressed"
        android:state_focused="true"
        android:state_enabled="true"
        android:state_pressed="true" />
    <item android:drawable="@drawable/ic_dash_training_flashcard_disabled"
        android:state_enabled="false"
       />
    <item android:drawable="@drawable/ic_dash_training_flashcard_pressed"
        android:state_focused="false"
        android:state_enabled="true"
        android:state_pressed="true" />
    <item android:drawable="@drawable/ic_dash_training_flashcard_selected" 
            android:state_enabled="true"
            android:state_focused="true" />
    <item android:drawable="@drawable/ic_dash_training_flashcard_default"
        android:state_enabled="true"
        android:state_focused="false"
        android:state_pressed="false" />
</selector>
